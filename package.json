{
	"name": "svelte-anime",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev --host",
		"build": "svelte-kit build",
		"build:config": "tsc -m commonjs --allowJs ./tests/utils/numbers.cjs ./tests/utils/css.ts --outDir ./config && node ./scripts/rename.cjs",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview --host",
		"prepare": "svelte-kit sync",
		"publish": "wrangler publish",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:watch": "chokidar --initial tests/e2e/**/*{spec,test}.{js,ts} -c \"pnpm run test:e2e\"",
		"coverage": "vitest run --coverage",
		"up": "pnpm up --interactive"
	},
	"devDependencies": {
		"@playwright/test": "^1.22.0-alpha-apr-9-2022",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-cloudflare-workers": "^1.0.0-next.36",
		"@sveltejs/kit": "1.0.0-next.311",
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.41",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"@vitest/ui": "^0.8.5",
		"autoprefixer": "^10.4.4",
		"c8": "^7.11.0",
		"chokidar-cli": "^3.0.0",
		"concurrently": "^7.1.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"jsdom": "^19.0.0",
		"postcss": "^8.4.12",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.47.0",
		"svelte-check": "^2.6.0",
		"svelte-preprocess": "^4.10.5",
		"tailwindcss": "^3.0.23",
		"tslib": "^2.3.1",
		"typescript": "~4.6.3",
		"vite": "^2.8.6",
		"vitest": "^0.8.5"
	},
	"type": "module"
}
